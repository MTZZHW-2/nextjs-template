{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:init": "sh server/dev-init.sh",
    "dev": "sh server/dev.sh && next dev",
    "build": "npm run prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "dotenv -e .env.production -- npx prisma generate --schema server/model/schema.prisma",
    "prisma:generate:dev": "dotenv -e .env.development -- npx prisma generate --schema server/model/schema.prisma",
    "prisma:migrate": "dotenv -e .env.production -- npx prisma migrate deploy --schema server/model/schema.prisma",
    "prisma:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --schema server/model/schema.prisma"
  },
  "dependencies": {
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.7.0",
    "@radix-ui/react-slot": "^1.2.2",
    "ahooks": "^3.8.4",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucia": "^3.2.2",
    "lucide-react": "^0.510.0",
    "minio": "^8.0.5",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "use-context-selector": "^2.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.26.0",
    "@next/eslint-plugin-next": "^15.3.2",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/qs": "^6.9.18",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.7.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5"
  },
  "engines": {
    "node": "22.x"
  }
}
